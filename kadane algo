class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int cur_sum =0;
        int max_sum= INT_MIN;
        for(int st=0;st<nums.size();st++){
            cur_sum +=nums[st];

            max_sum=max(cur_sum,max_sum);

            if(cur_sum<0){
                cur_sum =0;
            }

        }
        return max_sum;
    }
};
