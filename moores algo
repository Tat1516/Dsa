class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int fre=0;
        int answer=0;

        for(int i=0;i<nums.size();i++){
            if(fre==0){
                answer = nums[i];
            }
           if(answer == nums[i]){
            fre++;
           }
           else{
            fre--;
           }
        }
        return answer;
    }
};
