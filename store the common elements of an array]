#include<iostream>
using namespace std;

int main() {
    int arr1[] = {4, 5, 6, 7, 8, 23, 45, 65, 4};
    int arr2[] = {4, 5, 6, 7, 23, 45, 65, 9, 4};
    int mar[9];
    int i, j, k = 0;


    for(i = 0; i < 9; i++) {
        for(j = 0; j < 9; j++) {
            if(arr1[i] == arr2[j]) {
                mar[k] = arr1[i];
                k++;
                break;  
            }
        }
    }

  
    for(int r = 0; r < k; r++) {
        for(int s = r + 1; s < k; s++) {
            if(mar[r] == mar[s]) {
               
                for(int t = s; t < k - 1; t++) {
                    mar[t] = mar[t + 1];
                }
                k--;    
                s--;    
            }
        }
    }

    for(int l = 0; l < k; l++) {
        cout << "Common element: " << mar[l] << endl;
    }

    return 0;
}
