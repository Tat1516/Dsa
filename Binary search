#include<stdio.h>
using namespace std;
int main(){
    int tar = 123;
    int nums[] = {1,2,3,4,5,6,7,8,123};
    int n = 9;
    int mid,st = 0,en = n-1;

    while(st<=en){
            mid = st + (en-st)/2;
        if(nums[mid]<tar){
            st=mid+1;
        }
        else if(nums[mid]>tar){
            en=mid-1;
        }
        else
            return mid;
    }

    return -1;
}
